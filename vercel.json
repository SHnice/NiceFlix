{
    "version": 2,
    "builds": [
      { "src": "frontend/build/**", "use": "@vercel/static" },
      { "src": "backend/**", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "backend/index.js" },
      { "src": "/(.*)", "dest": "frontend/build/index.html" }
    ]
  }
  